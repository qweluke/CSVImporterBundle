<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="qweluke_csv_importer_import" path="/import-csv" methods="GET|POST">
        <default key="_controller">QwelukeCSVImporterBundle:Import:index</default>
    </route>
    <route id="qweluke_csv_importer_bind" path="/bind-columns" methods="GET|POST">
        <default key="_controller">QwelukeCSVImporterBundle:Import:bindColumns</default>
    </route>
</routes>
